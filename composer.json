{
    "name": "cruzio/netbox",
    "description": "API to interface with Netbox server",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.8",
        "monolog/monolog": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Cruzio\\lib\\Netbox\\": "src/",
            "Cruzio\\lib\\Env\\": "../Env/",
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Colin Miller",
            "email": "ocolin@cruzio.com"
        }
